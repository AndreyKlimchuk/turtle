{application, 'turtle',
 [{description, "Turtle manages RabbitMQ connections"},
  {vsn, "0.2.0"},
  {registered, []},
  {mod, {'turtle_app', []}},
  {applications,
   [kernel,
    stdlib,

    amqp_client,
    exometer_core,
    gproc,
    lager,
    msgpack,
    uuid
   ]},
  {env,[
      {gproc_env_order, [os_env, app_env, error]},
      {connection_config, [#{ conn_name => amqp_server }]}
  ]},
  {modules, []}
 ]}.
