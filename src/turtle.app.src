{application, 'turtle',
 [{description, "Turtle manages RabbitMQ connections"},
  {vsn, "0.1.0"},
  {registered, []},
  {mod, {'turtle_app', []}},
  {applications,
   [kernel,
    stdlib,
    lager,
    amqp_client,
    gproc,
    exometer_core
   ]},
  {env,[
      {gproc_env_order, [app_env, error]},
      {connection_config, [#{ conn_name => amqp_server }]}
  ]},
  {modules, []}
 ]}.
